<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping>
    <!-- 
    Created for Coadunation Principal
-->
    
    <class 
        name="com.rift.coad.auth.db.CoadunationPrincipal" 
        table="CoadunationPrincipal"
        proxy="com.rift.coad.auth.db.CoadunationPrincipal"
    >
        
        <id
            name="name"
            type="java.lang.String"
            column="name"
            length="50"
        >
            <generator class="assigned" />
        </id>
        
    </class>
</hibernate-mapping>
