<gadgets>
    
    <gadget identifier="UserGadget" 
        name="UserGadget" url=""
        width="0" height="0"/>
    
    <gadget identifier="time"
        name="time" url="/time/app"
        width="10" height="20">
        <app url="/time/clock"
            height="25"
            width="100"
            popup="true"
            popupURL="/time/popup"
            popupHeight="200"
            popupWidth="201"/>
    </gadget>

</gadgets>
