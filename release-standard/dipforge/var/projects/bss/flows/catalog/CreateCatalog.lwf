/**
 * This is the flow used to create the catalog
 */
@java("out")
@java("log")
@rdf("http://dipforge.sourceforge.net/schema/rdf/1.0/bss/Catalog#Catalog" "Catalog")
@store("store")
flow CreateCatalog {
   def out
   def log
   def Catalog

   {
       store.persist(Catalog)
       log.info("The catalog [" + Catalog.name + "] has been persisted")
   }
}
