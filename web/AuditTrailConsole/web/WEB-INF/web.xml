<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>TreeQuery</servlet-name>
        <servlet-class>com.rift.coad.audit.gwt.console.server.tree.TreeQueryImpl</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LogQuery</servlet-name>
        <servlet-class>com.rift.coad.audit.gwt.console.server.query.LogQueryImpl</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>TreeQuery</servlet-name>
        <url-pattern>/tree/treequery</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LogQuery</servlet-name>
        <url-pattern>/query/logquery</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>welcomeGWT.html</welcome-file>
    </welcome-file-list>
    <security-constraint>
        <display-name>admin</display-name>
        <web-resource-collection>
            <web-resource-name>admin</web-resource-name>
            <description>administration console</description>
            <url-pattern>/</url-pattern>
            <http-method>GET</http-method>
            <http-method>POST</http-method>
            <http-method>HEAD</http-method>
            <http-method>PUT</http-method>
            <http-method>OPTIONS</http-method>
            <http-method>TRACE</http-method>
            <http-method>DELETE</http-method>
        </web-resource-collection>
        <auth-constraint>
            <description/>
            <role-name>admin</role-name>
        </auth-constraint>
    </security-constraint>
    <login-config>
        <auth-method>BASIC</auth-method>
        <realm-name>Coadunation Administration Console</realm-name>
    </login-config>
    <security-role>
        <description>admin</description>
        <role-name>admin</role-name>
    </security-role>
</web-app>
