<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://www.netbeans.org/ns/project/1">
    <type>org.netbeans.modules.web.project</type>
    <configuration>
        <data xmlns="http://www.netbeans.org/ns/web-project/3">
            <name>CoadunationDesktop</name>
            <minimum-ant-version>1.6</minimum-ant-version>
            <web-module-libraries>
                <library>
                    <file>${file.reference.gwm.jar}</file>
                    <path-in-war>WEB-INF/lib</path-in-war>
                </library>
                <library>
                    <file>${file.reference.gwt-widgets-0.2.0.jar}</file>
                    <path-in-war>WEB-INF/lib</path-in-war>
                </library>
                <library>
                    <file>${file.reference.gwt-dev-linux.jar}</file>
                </library>
                <library>
                    <file>${file.reference.gwt-servlet.jar}</file>
                    <path-in-war>WEB-INF/lib</path-in-war>
                </library>
                <library>
                    <file>${reference.DesktopServerClient.jar}</file>
                    <path-in-war>WEB-INF/lib</path-in-war>
                </library>
                <library>
                    <file>${file.reference.log4j-1.2.13.jar}</file>
                    <path-in-war>WEB-INF/lib</path-in-war>
                </library>
                <library>
                    <file>${file.reference.gwt-user.jar}</file>
                    <path-in-war>WEB-INF/lib</path-in-war>
                </library>
                <library>
                    <file>${reference.EventServerClient.jar}</file>
                    <path-in-war>WEB-INF/lib</path-in-war>
                </library>
                <library>
                    <file>${file.reference.CoadunationUtil.jar}</file>
                    <path-in-war>WEB-INF/lib</path-in-war>
                </library>
                <library>
                    <file>${file.reference.CoadunationLib.jar}</file>
                </library>
            </web-module-libraries>
            <web-module-additional-libraries/>
            <source-roots>
                <root id="src.dir"/>
            </source-roots>
            <test-roots>
                <root id="test.src.dir"/>
            </test-roots>
        </data>
        <references xmlns="http://www.netbeans.org/ns/ant-project-references/1">
            <reference>
                <foreign-project>DesktopServerClient</foreign-project>
                <artifact-type>jar</artifact-type>
                <script>build.xml</script>
                <target>jar</target>
                <clean-target>clean</clean-target>
                <id>jar</id>
            </reference>
            <reference>
                <foreign-project>EventServerClient</foreign-project>
                <artifact-type>jar</artifact-type>
                <script>build.xml</script>
                <target>jar</target>
                <clean-target>clean</clean-target>
                <id>jar</id>
            </reference>
        </references>
    </configuration>
</project>
